{
  "typescript.referencesCodeLens.enable": false,
  "javascript.implementationsCodeLens.enable": false,
  "javascript.referencesCodeLens.enable": false,
  "suggest.enablePreview": true,
  "diagnostic.warningSign": "⚠",
  "diagnostic.errorSign": "✕",
  "diagnostic.infoSign": "✕",
  "diagnostic.hintSign": "✕",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualTextPrefix": "✕ ",
  "coc.preferences.floatActions": true,
  "coc.preferences.messageLevel": "warning",
  "coc.preferences.formatOnSaveFiletypes": [
    "go",
    "javascript",
    "typescript",
    "typescriptreact"
  ],
  "coc.source.buffer.enable": false,
  "signature.preferShownAbove": true,
  "codeLens.enable": true,
  "hover.target": "float",
  "suggest.disableMenu": true,
  "suggest.detailField": "preview",
  "suggest.noselect": false,
  "suggest.removeDuplicateItems": true,
  "typescript.format.enabled": false,
  "javascript.format.enabled": false
}
