{
  "[erb]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[git-commit]": {
    "editor.rulers": [
      50,
      72
    ]
  },
  "[ruby]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "breadcrumbs.enabled": false,
  "breadcrumbs.icons": false,
  "debug.javascript.autoAttachFilter": "onlyWithFlag",
  "debug.showInlineBreakpointCandidates": false,
  "editor.cursorSurroundingLines": 8,
  "editor.folding": false,
  "editor.fontFamily": "Hack, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": false,
  "editor.formatOnSave": true,
  "editor.glyphMargin": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.highlightActiveIndentGuide": false,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.overviewRulerBorder": false,
  "editor.rulers": [],
  "editor.scrollbar.horizontalScrollbarSize": 6,
  "editor.scrollbar.verticalScrollbarSize": 6,
  "editor.suggest.preview": true,
  "editor.suggest.showIcons": false,
  "everforest.darkContrast": "hard",
  "everforest.darkWorkbench": "high-contrast",
  "explorer.incrementalNaming": "smart",
  "explorer.openEditors.sortOrder": "alphabetical",
  "explorer.openEditors.visible": 0,
  "files.enableTrash": false,
  "files.insertFinalNewline": true,
  "git.autofetch": true,
  "git.inputValidation": "always",
  "git.suggestSmartCommit": false,
  "gitlens.blame.heatmap.enabled": false,
  "gitlens.blame.highlight.enabled": false,
  "gitlens.blame.separateLines": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.defaultGravatarsStyle": "retro",
  "gitlens.gitCommands.closeOnFocusOut": false,
  "gitlens.gitCommands.search.matchRegex": false,
  "gitlens.hovers.avatars": false,
  "gitlens.liveshare.allowGuestAccess": false,
  "gitlens.menus": false,
  "gitlens.mode.statusBar.enabled": false,
  "gitlens.showWelcomeOnInstall": false,
  "gruvboxMaterial.darkWorkbench": "high-contrast",
  "material-icon-theme.opacity": 0.6,
  "material-icon-theme.saturation": 1,
  "notebook.breadcrumbs.showCodeCells": false,
  "redhat.telemetry.enabled": false,
  "scm.alwaysShowRepositories": true,
  "scm.autoReveal": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "timeline.excludeSources": [
    "git-history"
  ],
  "vim.hlsearch": true,
  "vim.leader": " ",
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "<leader>"
      ],
      "commands": [
        ":noh"
      ]
    }
  ],
  "vim.showmodename": false,
  "vim.timeout": 0,
  "workbench.colorTheme": "Gruvbox Material Dark",
  "vim.visualstar": true,
  "workbench.activityBar.visible": false,
  "workbench.colorCustomizations": {
    "scrollbar.shadow": "#ffffff00"
  },
  "workbench.editor.showIcons": false,
  "workbench.editor.tabCloseButton": "off",
  "workbench.enableExperiments": false,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none"
}
